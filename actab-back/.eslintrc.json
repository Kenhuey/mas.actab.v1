{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "indent": "off",
        "linebreak-style": "off",
        "quotes": [
            1,
            "double"
        ],
        "import/extensions": "off",
        "comma-dangle": "off",
        "max-classes-per-file": "off",
        "object-curly-newline": "off",
        "no-control-regex": "off",
        "global-require": "off",
        "no-param-reassign": "off",
        "class-methods-use-this": "off",
        "operator-linebreak": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "warn"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".ts"
                ]
            }
        }
    }
}